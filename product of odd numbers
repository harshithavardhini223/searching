def product_of_odd(m, n):
    if m > n:
        return 0  # invalid range
    
    product = 1
    found_odd = False  # to check if at least one odd number exists

    for i in range(m, n + 1):
        if i % 2 != 0:  # check odd
            product *= i
            found_odd = True
    
    if not found_odd:
        return 0  # no odd numbers in range
    return product

# --- main program ---
m = int(input("Enter m: "))
n = int(input("Enter n: "))

result = product_of_odd(m, n)

if result == 0:
    if m > n:
        print("Invalid range! m should be less than or equal to n.")
    else:
        print("No odd numbers found in the given range.")
else:
    print(f"Product of odd numbers from {m} to {n} = {result}")
